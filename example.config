server {
    host 127.73.73.7;
	listen 7681;
	server_name 127.73.73.7; 
    root /guarder-html;
    index index.html;
    client_max_body_size 50;
    autoindex on;
    cgi .php ./cgi-bin/;
	error_page 404 404.html;
	error_page 430 430.html;

    location /js/ {
        allow GET POST DELETE;
		error_page 404 403.html;
    }

    location /images/ {
        allow GET POST DELETE;
        error_page 404 403.html;
    }

    location / {
        root /guarder-html;
        index /index.html;
        allow GET POST DELETE;
        error_page 404 403.html;
    }

    location /css/ {
        allow GET DELETE;
    }
}

